4(a)
#include <stdio.h>

void
main ()
{
  int x = 0, sum;

  do
    {
      sum = sum + x;
      x++;
    }
  while (x < 11);
  printf ("the sum is %d", sum);
}

/*4(b)*/

#include <stdio.h>

int main ()
{
  int i, sum = 0;

  for (i=2;i<=20;i=i+2)
    {
      sum = sum + i;
      
    }

  printf ("The sum is %d", sum);

  return 0;
}
