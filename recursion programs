// finding fibo sequence
#include <stdio.h>

int fibo(int x);
int main()
{
    int n,i,hold;
    printf("Please input a limit\n");
    scanf("%d",&n);
    
    for(i=0;i<n;i++)
    hold=fibo(i);
    
    
    printf("%d",hold);
}
int fibo(int x)
{
    if (x == 0 | x == 1)
    return x;
    else 
    return(fibo(x-2)+fibo(x-1));
}

// finding factorial
